{
  "expo": {
    "sdkVersion": "53.0.0",
    "name": "Katalog",
    "slug": "Katalog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/katalog.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/katalog.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/katalog.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/katalog.png"
    },
    "plugins": [
      "expo-sqlite",
      ["expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
    ]
  ]
  }
}
